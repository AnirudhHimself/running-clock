  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "3.1.2",
    "@tsconfig/svelte": "5.0.4",
    "@types/node": "22.5.1",
    "@typescript-eslint/eslint-plugin": "8.3.0",
    "eslint": "9.9.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "2.43.0",
    "husky": "9.1.5",
    "prettier": "3.3.3",
    "prettier-plugin-svelte": "3.2.6",
    "sass": "1.77.8",
    "svelte": "4.2.19",
    "svelte-check": "3.8.6",
    "svelte-preprocess": "6.0.2",
    "tslib": "2.7.0",
    "typescript": "5.5.4",
    "vite": "5.4.6"
  },
  "prettier": {
    "arrowParens": "avoid",
