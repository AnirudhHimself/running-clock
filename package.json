{
  "name": "running-clock",
  "version": "0.0.0",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "format": "prettier --write .",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "serve": "vite preview",
    "typecheck": "tsc --noEmit",
    "postinstall": "husky install"
  },
  "dependencies": {
    "react": "~18.0.0",
    "react-dom": "~18.0.0"
  },
  "devDependencies": {
    "@types/node": "~16.11.26",
    "@types/react": "~18.0.0",
    "@types/react-dom": "~18.0.0",
    "@vitejs/plugin-react": "~1.3.0",
    "husky": "^7.0.0",
    "pinst": "^3.0.0",
    "prettier": "~2.6.2",
    "sass": "~1.50.0",
    "typescript": "~4.6.3",
    "vite": "~2.9.1"
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "packageManager": "yarn@3.2.0"
}
